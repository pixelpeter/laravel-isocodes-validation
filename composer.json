{
  "name": "pixelpeter/laravel-isocodes-validation",
  "description": "Laravel 8+ wrapper for the IsoCodes Validation library from ronanguilloux",
  "keywords": [
    "laravel",
    "iso",
    "isocode",
    "validation"
  ],
  "homepage": "https://github.com/pixelpeter/laravel-isocodes-validation",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Peter Haak",
      "email": "info@pixelpeter.de",
      "homepage": "https://pixelpeter.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.0|^11.0",
    "ronanguilloux/isocodes": "^2.3.5"
  },
  "autoload": {
    "psr-4": {
      "Pixelpeter\\IsoCodesValidation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pixelpeter\\IsoCodesValidation\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.0",
    "mockery/mockery": "^1.2",
    "php-coveralls/php-coveralls": "^2.7",
    "phpstan/extension-installer": "^1.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pixelpeter\\IsoCodesValidation\\IsoCodesValidationServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  }
}